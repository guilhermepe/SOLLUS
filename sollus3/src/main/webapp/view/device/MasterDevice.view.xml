<mvc:View
	controllerName="com.guiper.sollus.controller.device.MasterDevice"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

	<Page id="masterDevicePage"
		showHeader="false">
		<List
			id="masterDeviceList"
			items="{/Equipamentos}">
			<headerToolbar>
				<Toolbar>
					<Title text="Equipamentos"/>
					<ToolbarSpacer/>
					<SearchField width="50%" search="onFilterProducts"/>
				</Toolbar>
			</headerToolbar>			
			<items>
				<ObjectListItem
					title="{Nome}"
					number="{Numeroserie}"
					intro="{DataHoraUltimaLeitura}" />
			</items>
		</List>
	</Page>
</mvc:View>